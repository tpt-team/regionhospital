<div class="contact-form">
  <div class='container'>
    <div class="col-md-4 pd adress">
      <h3><%=t('.address')%></h3>
      <ul>
        <li><%=t('.city')%></li>
        <li><%=t('.country')%></li>
        <li><%=t('.street')%></li>
        <li><%=t('.phone_department')%></li>
        <li><%=t('.phone_department_head')%></li>
        <li><%=t('.department_number')%></li>

      </ul>
      </div>
      <div class="col-md-8 pd contact-us">
        <h3><%=t('.contact_us')%></h3>
        <form>
          <textarea value="Message:" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Message';}"><%= t('.write_text_here')%></textarea>
          <br>
          <input class="name" type="text" name="name" value=<%= t('.your_name')%> onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Name';}">
          <input class="nuber" type="text" name="Phone number" value=<%= t('.number')%> onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Number';}">
          <button type="button" class="btn btn-info sub1"><%= t('.send')%></button>
        </form>
      </div>
      <div class="clearfix"></div>
    </div>
  </div>
</div>
